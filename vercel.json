{
  "version": 2,
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "ssr": {
        "noExternal": ["node:process"]
      }
    }
  ]
}
